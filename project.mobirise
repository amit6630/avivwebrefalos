          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer1\" group=\"Footer\" mbr-class=\"{'mbr-reveal': reveal, 'mbr-parallax-background': bg.parallax}\" always-bottom global once=\"footer\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n        <input type=\"checkbox\" title=\"Reveal effect\" name=\"reveal\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Brand\" name=\"showBrand\" checked>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\">\n        <input type=\"range\" title=\"Logo Size\" inline name=\"logoSize\" min=\"2\" max=\"5\" step=\"0.1\" value=\"3\" condition=\"showLogo\">\n        <input type=\"checkbox\" title=\"Show Bottom\" name=\"showBottom\" checked>\n        <select name=\"countMenu\" title=\"Count Menu\">\n            <option value=\"0\">None</option>\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n        </select>\n        <input type=\"range\" title=\"Item 1\" name=\"countItem1\" min=\"1\" max=\"10\" step=\"1\" value=\"1\" condition=\"countMenu > '0'\">\n        <input type=\"range\" title=\"Item 2\" name=\"countItem2\" min=\"1\" max=\"10\" step=\"1\" value=\"5\" condition=\"countMenu > '1'\">\n        <input type=\"range\" title=\"Item 3\" name=\"countItem3\" min=\"1\" max=\"10\" step=\"1\" value=\"3\" condition=\"countMenu > '2'\">\n        <input type=\"range\" title=\"Item 4\" name=\"countItem4\" min=\"1\" max=\"10\" step=\"1\" value=\"3\" condition=\"countMenu > '3'\">\n        <input type=\"color\" title=\"Line\" value=\"#fef2ed\" name=\"lineColor\" condition=\"showBottom\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/newsm4/components/_images/background.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#078efb\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"align-center\" mbr-class=\"{'container' : !fullWidth, 'container-fluid' : fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-12 col-md-12\" mbr-if=\"showBrand || showLogo\">\n                <div class=\"brand_wrap\">\n                     <img class=\"footer_logo\" mbr-if=\"showLogo\" src=\"@ADDONS_PATH@/newsm4/components/_images/logo.png\" alt=\"Mobirise\" mbr-style=\"{'height': logoSize + 'rem'}\" title>\n                   <p class=\"footer_caption mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showBrand\" data-app-selector=\".footer_caption\">האתר</p>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-3\" mbr-if=\"countMenu > 0\">\n                <div class=\"footer_wrap mbr-white\">\n                    <h4 class=\"mbr-fonts-style mbr-semibold footer_title\" mbr-theme-style=\"display-7\" data-app-selector=\".footer_title\">לפרטים נוספים</h4>\n                    <div mbr-list mbr-list-grow=\"countItem1\">\n                        <p class=\"footer_items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer_items\"><a href=\"tel:+972535303949\" class=\"text-white\"><b>0535303949</b></a><br><a href=\"privacy.html\" class=\"text-primary\">מדיניות פרטיות</a><br><a href=\"usingtheweb.html\" class=\"text-primary\">תנאי שימוש<br></a><br></p>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-3\" mbr-if=\"countMenu > 1\">\n                <div class=\"footer_wrap mbr-white\">\n                    <h4 class=\"mbr-fonts-style mbr-semibold footer_title\" mbr-theme-style=\"display-7\" data-app-selector=\".footer_title\">ניווט מהיר</h4>\n                    <div mbr-list mbr-list-grow=\"countItem2\">\n                        <p class=\"footer_items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer_items\"><a href=\"index.html#header17-0\" class=\"text-primary\"><b>בית</b></a></p>\n                    <p class=\"footer_items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer_items\"><a href=\"page2.html#gallery04-d\" class=\"text-primary\"><b>גלריה</b></a></p><p class=\"footer_items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer_items\"><a href=\"page1.html\" class=\"text-primary\"><b>אודות</b></a></p><p class=\"footer_items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer_items\"><a href=\"index.html#features09-3\" class=\"text-white\"><b>השירותים שלנו</b></a></p><p class=\"footer_items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer_items\"><a href=\"index.html#form01-6\" class=\"text-primary\"><b>צרו קשר</b></a></p></div>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-3\" mbr-if=\"countMenu > 2\">\n                <div class=\"footer_wrap mbr-white\">\n            